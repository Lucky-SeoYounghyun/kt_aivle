# 2차원 배열 만들기
score= np.array([[78, 91, 84, 89, 93, 65],
                 [82, 87, 96, 79, 91, 73]])

# 확인
print(score)





# 요소 중에서 90 이상인 것만 조회
print(score[score >= 90])





# 요소 중에서 90 이상인 것만 조회
condition = score >= 90
print(score[condition])





# 모든 요소 중에서 90 이상 95 미만인 것만 조회
print(score[(score >= 90) & (score <= 95)])








# 2차원 배열 만들기
score= np.array([[78, 91, 84, 89, 93, 65],
                 [82, 87, 96, 79, 91, 73]])

# 확인
print(score)

# 80 미만 데이터 조회
a = score[score < 80]
print(a)





# 2차원 배열 만들기
score= np.array([[78, 91, 84, 89, 93, 65],
                 [82, 87, 96, 79, 91, 73]])

# 확인
print(score)
# 짝수만 조회
print(score[score % 2 == 0])








