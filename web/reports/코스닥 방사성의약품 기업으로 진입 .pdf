				<script>
					window.addEventListener('message', function(event) {
						if (event.origin !== location.protocol+'//'+location.host) return;
						var data = event.data;

						switch (data.type) {
							case "sendDataByCompanySearch": // iframe 높이 조절
								document.search_form.itemCode.value = data.itemCode;
								document.search_form.itemName.value = data.itemName;
								break;
						}
					});

					function checkForm(frm)
					{
						if(frm.searchType[0].checked && frm.keyword.value == '')
						{
							alert('제목+내용을 입력하세요.');
							frm.keyword.focus();
							return false;
						}
						else if(frm.searchType[1].checked && frm.brokerCode.selectedIndex < '1')
						{
							alert('증권사를 선택하세요.');
							frm.brokerCode.focus();
							return false;
						}
						else if(frm.searchType[2].checked)
						{
							var ymdFrom = frm.writeFromDate.value.replace(/-/g,"");
							var ymdTo = frm.writeToDate.value.replace(/-/g,"");

							if(!(ymdFrom.length==8))
							{
								alert('시작 기간 날짜형식이 잘못되었습니다.')
								frm.writeFromDate.focus();
								return false;
							}

							if(!(ymdTo.length==8))
							{
								alert('끝나는 기간 날짜형식이 잘못되었습니다.')
								frm.writeToDate.focus();
								return false;
							}

							if(ymdFrom > ymdTo)
							{
								alert('시작 기간은 끝나는 기간보다 작거나 같아야 합니다.')
								frm.writeFromDate.focus();
								return false;
							}

							var yFrom = ymdFrom.substr(0,4);
							var mFrom = ymdFrom.substr(4,2)-1;
							var dFrom = ymdFrom.substr(6,2);
							var yTo = ymdTo.substr(0,4);
							var mTo = ymdTo.substr(4,2)-1;
							var dTo = ymdTo.substr(6,2);

							var vDateFrom = new Date(yFrom,mFrom,dFrom);
							if( vDateFrom.getFullYear() != yFrom || vDateFrom.getMonth() != mFrom || vDateFrom.getDate() != dFrom )
							{
								alert('시작 기간 날짜형식이 잘못되었습니다.')
								frm.writeFromDate.focus();
								return false;
							}

							var vDateTo = new Date(yTo,mTo,dTo);
							if( vDateTo.getFullYear() != yTo || vDateTo.getMonth() != mTo || vDateTo.getDate() != dTo )
							{
								alert('끝나는 기간 날짜형식이 잘못되었습니다.')
								frm.writeToDate.focus();
								return false;
							}
						}

						if(!frm.searchType[0].checked)
						{
							frm.keyword.value = '';
						}

						if(!frm.searchType[2].checked)
						{
							frm.writeFromDate.value = '';
							frm.writeToDate.value = '';
						}

						if(!frm.searchType[3].checked)
						{
							frm.itemName.value = '';
						}

						return true;
					}

